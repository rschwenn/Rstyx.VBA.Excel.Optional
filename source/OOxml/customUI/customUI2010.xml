<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="OnHooksRibbonLoad">
  <!-- ACHTUNG: "commands" muss vor "ribbon" stehen!!! -->
  
  <commands>
    <command idMso="FileNewDefault" onAction="FromFileNewDefault"/>
  </commands>
  
  <ribbon>
    <tabs>
      <tab id="SpecialTab" label="Spezial">
        <group id="HooksGroup" label="Hooks">
          <button id="HooksSettingsButton"
                  label="Einstellungen spezial"
                  size="large"
                  onAction="SettingsButtonAction"
                  imageMso="AddInManager" />
        </group>
      </tab>
    </tabs>
  </ribbon>
  
  <contextMenus>
    <contextMenu idMso="ContextMenuCell">
      <menuSeparator id="MySeparator" />
      <button idMso="ConditionalFormattingsManage"
              getVisible="getVisibleConditionalFormat"
              label="Bedingte Formatierung"
              insertBeforeMso="FormatCellsDialog" />
    </contextMenu>
  </contextMenus>
  
</customUI>
